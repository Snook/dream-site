{
  "name": "dream-dinners",
  "homepage": "https://dreamdinners.com",
  "private": true,
  "scripts": {
    "css": "npm-run-all css-compile css-minify",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css www/theme/dreamdinners/css/customer:www/theme/dreamdinners/css/customer",
    "css-minify": "npm-run-all --aggregate-output --parallel css-minify-*",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output www/theme/dreamdinners/css/customer/ --batch --batch-suffix \".min\" \"www/theme/dreamdinners/css/customer/*.css\" \"!www/theme/dreamdinners/css/customer/*.min.css\" \"!www/theme/dreamdinners/css/customer/*rtl*.css\""
  },
  "dependencies": {
    "@types/bootstrap": "^5.1.12",
    "bootbox": "^5.5.3",
    "bootstrap": "^4.6.1",
    "cleave.js": "^1.6.0",
    "clipboard": "^2.0.11",
    "fullcalendar": "^5.5.1",
    "hellojs": "^1.19.5",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "jquery-uitablefilter": "^1.0.0",
    "jquery.cookie": "^1.4.1",
    "jstree": "^3.3.12",
    "moment": "^2.29.3",
    "qtip2": "^3.0.3"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.8",
    "postcss-cli": "^9.0.1",
    "postcss-loader": "^6.2.1",
    "sass": "^1.52.1",
    "sass-loader": "^12.6.0",
    "terser": "^5.9.0",
    "uglify-js": "^3.15.5"
  }
}
